  flex-direction: column;
  justify-content: center;
}
.titelContainer{
  display: flex;
}

.englishChineseContainer{
  display: flex;
  flex-direction: column;
  align-items: center;    
  vertical-align: center;
  justify-content: center;
  margin-left: 20rpx;
}

.schoolSign{
  width: 260rpx;
  height: 130rpx;
  margin: 0rpx 0;
}


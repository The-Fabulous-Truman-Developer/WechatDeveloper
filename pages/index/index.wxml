<view class="indexContainer">
  <view class="titelContainer">
     <image class="schoolSign" src="/static/images/TrumanSignTitle.png"></image>
     <text class="chineseSchoolName">杜鲁门州立大学信息平台</text>
  </view>

<swiper>
    <swiper-item wx:for="{{imgUrls}}" wx:key="*this">
      <image src='{{item}}' width="335" height="150" mode='widthFix' class='img' />
    </swiper-item>
</swiper>
</view>  

<view class="page">
    <view class="page__hd">
    </view>
    <mp-tabbar style="position:fixed;bottom:0;width:100%;left:0;right:0;" list="{{list}}" bindchange="tabChange"></mp-tabbar>
</view>